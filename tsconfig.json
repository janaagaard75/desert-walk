{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "build/dist",
    "skipLibCheck": true, // Temporarely needed by @types/react-native 0.52.22. See https://github.com/DefinitelyTyped/DefinitelyTyped/issues/24573
    "sourceMap": false,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "es2017",
    "types": [
      "react",
      "react-native"
    ],
  },
  "exclude": [
    "node_modules"
  ],
  "types": [
    "typePatches"
  ]
}